{
  "title": "未完",
  "author": "未完",
  "description": "未完的博客",
  "language": "zh-hans",
  "styles": {
    "website": "./public-repertory/css/gitbook-configure.css"
  },
  
  "plugins": [
    "livereload",
    "theme-comscore",
    "code",
    "-highlight",
    "prism",
    "prism-themes",
    
    "search-pro",
    "-search",
    "-lunr",
    "expandable-chapters",
    "splitter",
    "-sharing",
    "sharing-plus",
    "-donate",
    "tbfed-pagefooter",
    
    "hide-element",
    "lightbox",
    "favicon-absolute",
    "-back-to-top-button",
    "anchor-navigation-ex",
    "pageview-count",
    "github"
  ],
  "pluginsConfig": {

     "sharing": {
            "douban": false,
            "facebook": false,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": false,
            "viber": false,
            "vk": false,
            "weibo": false,
            "whatsapp": false,
            "all": [
                
            ]
        },

     "hide-element": {
            "elements": [".gitbook-link"]
     },
    
    "donate": {
      "button": "打赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏",
      "alipay": "https://github.com/LiangJunrong/document-library/blob/master/public-repertory/img/seek-reward.jpg?raw=true",
      "wechat": "https://github.com/LiangJunrong/document-library/blob/master/public-repertory/img/seek-reward.png?raw=true"
    },
    "prism": {
      "css": [
        "prism-themes/themes/prism-one-dark.css"
      ]
    },
    "tbfed-pagefooter": {
      "copyright":"Copyright &copy iweiwan.github.io 2022",
      "modify_label": "该文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "favicon-absolute":{
            "favicon": "/_media/favicon.ico",
            "appleTouchIconPrecomposed152": "/_media/icon-144x144.png"
    },
     "lightbox": {
        "includeJQuery": false,
        "sameUuid": true,
        "options": {
            "resizeDuration": 500,
            "wrapAround": false
        }
    },
    "github": {
            "url": "https://github.com/iweiwan/iweiwan.github.io"
     }
  }
}
